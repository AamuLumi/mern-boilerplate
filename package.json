{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "description": "MERN (Mongo Express React Node) boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "npm run production",
    "dev": "pm2 start scripts/pm2_dev.json",
    "production": "npm run deploy && pm2 start scripts/pm2_production.json",
    "deploy": "set NODE_ENV=production && webpack -p --config app/config/webpack.production.config.js",
    "test": "mocha test/index.js",
    "stop": "pm2 stop all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AamuLumi/mern-boilerplate.git"
  },
  "keywords": [
    "mern",
    "react",
    "mongoose",
    "express",
    "node",
    "boilerplate"
  ],
  "author": "Florian Kauder <florian@kauder.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AamuLumi/mern-boilerplate/issues"
  },
  "homepage": "https://github.com/AamuLumi/mern-boilerplate#readme",
  "dependencies": {
    "async": "^2.1.2",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "css-loader": "^0.26.0",
    "eslint": "^3.10.2",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "jwt-simple": "^0.5.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.1.2",
    "mongoose": "^4.6.8",
    "nodemailer": "^2.6.4",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "^2.3.2",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
